{
  "name": "ez-site-ui-kit",
  "version": "0.0.2",
  "description": "ui toolkit for ezeewei company websites",
  "main": "dist/index.js",
  "scripts": {
    "p": "webpack -p",
    "pa": "webpack -p && npm run analyze:win",
    "prod": "set NODE_ENV=production npm run p && npm run analyze",
    "start": "webpack --env.watch=true",
    "start:p": "npm link ez-site-content-store && webpack -p --env.watch=true",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -s ./public -p 9001",
    "s": "npm link ez-site-content-store && npm run storybook",
    "build-storybook": "build-storybook",
    "analyze": "webpack --profile --json > stats.json",
    "analyze:win": "webpack --env-analyze=true --profile --json | Out-file 'stats.json' -Encoding OEM",
    "workspace": "npm-run-all --parallel start:p storybook"
  },
  "author": "Ezeewei",
  "license": "ISC",
  "devDependencies": {
    "@storybook/react": "^3.2.19",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "has-flag": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "npm-run-all": "^4.1.2",
    "storybook": "^1.0.0",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^4.0.0-beta.3",
    "ez-site-content-store": "git+ssh://git@bitbucket.org:adam_chen_wei/ez-site-content-store.git",
    "material-ui": "^1.0.0-beta.20",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-google-maps": "^9.4.5",
    "react-html-parser": "^2.0.2",
    "react-images": "^0.5.14",
    "react-router-dom": "^4.2.2",
    "react-share": "^1.18.1",
    "react-svg": "^2.2.6",
    "reactstrap": "^5.0.0-alpha.4",
    "styled-components": "^2.4.0"
  }
}
