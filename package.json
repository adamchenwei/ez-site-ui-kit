    "babel-cli": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-env": "^1.6.1",
