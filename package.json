{
  "name": "ez-site-ui-kit",
  "version": "0.0.2",
  "description": "ui toolkit for ezeewei company websites",
  "main": "dist/index.js",
  "scripts": {
    "p": "webpack -p",
    "pa": "webpack -p && npm run analyze:win",
    "prod": "set NODE_ENV=production npm run p && npm run analyze",
    "start": "webpack --env.watch=true",
    "start:p": "npm link ez-site-content-store && webpack -p --env.watch=true",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -s ./public -p 9001",
    "s": "npm link ez-site-content-store && npm run storybook",
    "build-storybook": "build-storybook",
    "analyze": "webpack --profile --json > stats.json",
    "analyze:win": "webpack --env-analyze=true --profile --json | Out-file 'stats.json' -Encoding OEM",
    "workspace": "npm-run-all --parallel start storybook",
    "lint:fix:custom": "./node_modules/eslint/bin/eslint.js --c ./.eslintrc --fix ./src",
    "lint:fix:custom:debugg": "./node_modules/eslint/bin/eslint.js --debug --c ./.eslintrc --fix ./src",
    "lint:fix": "./node_modules/eslint/bin/eslint.js --debug --fix ./src"
  },
  "author": "Ezeewei",
  "license": "ISC",
