{
  "name": "ez-site-ui-kit",
  "version": "0.0.2",
  "description": "ui toolkit for ezeewei company websites",
  "main": "dist/index.js",
  "scripts": {
    "p": "webpack -p",
    "pa": "webpack -p && npm run analyze:win",
    "prod": "set NODE_ENV=production npm run p && npm run analyze",
    "start": "webpack --env.watch=true",
    "start:p": "npm link ez-site-content-store && webpack -p --env.watch=true",
    "storybook": "start-storybook -s ./public -p 6006",
    "s": "npm link ez-site-content-store && npm run storybook",
    "build-storybook": "build-storybook",
    "analyze": "webpack --profile --json > stats.json",
    "analyze:win": "webpack --env-analyze=true --profile --json | Out-file 'stats.json' -Encoding OEM",
    "workspace": "npm-run-all --parallel start storybook",
    "lint:fix": "npm run link && ./node_modules/eslint/bin/eslint.js --c ./.eslintrc --fix ./src",
    "lint:fix:debugg": "./node_modules/eslint/bin/eslint.js --debug --c ./.eslintrc --fix ./src",
    "lint:fix:default": "./node_modules/eslint/bin/eslint.js --debug --fix ./src",
    "test": "./node_modules/jest/bin/jest.js --coverage",
    "test:watch": "npm run test -- --coverage",
    "test:updateSnapshot": "npm run link && ./node_modules/jest/bin/jest.js --updateSnapshot",
    "test:once": "./node_modules/jest/bin/jest.js --coverage",
    "cleanup": "npm run test:updateSnapshot && npm run lint:fix",
    "link": "npm link ez-site-content-store",
    "push:master": "npm run link && git push origin master"
  },
  "pre-push": [
    "test",
    "lint:fix"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!src/**/*.test.{js,jsx}",
      "!src/**/*.stories.{js,jsx}",
      "!src/proptypes/**/*.{js,jsx}",
      "!src/cosntants/**/*.{js,jsx}",
      "!src/prootypes/**/*.{js,jsx}",
      "!src/theme/**/*.{js,jsx}",
      "!src/storybookonly/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/src/__mocks__/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/image.js"
    },
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "testResultsProcessor": "./node_modules/jest-html-reporter"
  },
  "author": "Ezeewei",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-transform-destructuring": "^7.0.0-beta.44",
    "@babel/types": "^7.0.0-beta.42",
    "@storybook/addon-actions": "^3.4.0",
    "@storybook/addon-info": "^3.3.15",
    "@storybook/addon-knobs": "^3.3.15",
    "@storybook/addon-links": "^3.4.0",
    "@storybook/addon-notes": "^3.3.15",
    "@storybook/addon-storyshots": "^3.3.15",
    "@storybook/addons": "^3.4.0",
    "@storybook/react": "^3.4.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "commitizen": "^2.9.6",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.3",
    "has-flag": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.3",
    "jest-html-reporter": "^2.0.0",
    "jest-localstorage-mock": "^2.2.0",
    "moxios": "^0.4.0",
    "npm-run-all": "^4.1.2",
    "pre-push": "^0.1.1",
    "react-test-renderer": "^16.3.1",
    "style-loader": "^0.20.3",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^4.0.0-beta.3",
    "braintree-web-drop-in-react": "^1.0.3",
    "ez-site-content-store": "git+ssh://git@bitbucket.org:adam_chen_wei/ez-site-content-store.git",
    "html-react-parser": "^0.4.2",
    "lodash": "^4.17.5",
    "material-ui": "^1.0.0-beta.20",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dnd": "^2.6.0",
    "react-dom": "^16.2.0",
    "react-google-maps": "^9.4.5",
    "react-html-parser": "^2.0.2",
    "react-images": "^0.5.14",
    "react-lifecycles-compat": "^3.0.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-share": "^1.18.1",
    "react-svg": "^2.2.6",
    "react-table": "^6.8.0",
    "reactstrap": "^5.0.0-alpha.4",
    "styled-components": "^2.4.0"
  }
}
